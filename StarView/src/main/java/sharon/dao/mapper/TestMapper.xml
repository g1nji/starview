<?xml version="1.0" encoding="UTF-8"?>

<!-- 마이바티스 3 Mapper DTD -->
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sharon.dao.face.MyDao">

<update id="Modify" parameterType="ydg.dto.Users">
    update users
    set uPw = #{uPw}, uAddress={uAddress},uNick={uNick},uPhone={uPhone}, uEmail={uEmail}
    where uId = #{uId}
</update>

<update id="withDraw" parameterType="String">
    delete from users where uId = #{uId}
</update>



</mapper>

