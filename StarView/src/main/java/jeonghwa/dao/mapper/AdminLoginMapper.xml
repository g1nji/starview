<?xml version="1.0" encoding="UTF-8"?>

<!-- 마이바티스 3 Mapper DTD -->
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="jeonghwa.dao.face.AdminLoginDao">
    	<select id="selectCntByIdPW" parameterType="jeonghwa.dto.Users" resultType="int">
    		SELECT count(*) FROM users
    		WHERE u_id = #{uId } AND u_pw = #{uPw }
    	</select>
    </mapper>




